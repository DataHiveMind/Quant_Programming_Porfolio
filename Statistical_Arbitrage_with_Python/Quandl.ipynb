    "import quandl\n",
    "\n",
    "class QuandlData:\n",
    "    def __init__(self, api_key):\n",
    "        self.api_key = api_key\n",
    "    \n",
    "    def get_data(self, dataset_code):\n",
    "        quandl.ApiConfig.api_key = self.api_key\n",
    "        data = quandl.get(dataset_code)\n",
    "        return data\n"
